	article#teskoding
		h1 Tes Ngoding
		select#bahasa(name="PL")
			option(value="java") Java
			option(value="c++") C++
			option(value="python") Python
			option(value="ruby") Ruby
			option(value="javascript") Javascript
		button#run Jalankan
		div
			.label Input : 
			textarea#input-bahasa(name="input", rows="20")
		div
			.label Output : 
			textarea#output-bahasa(name="output", rows="10", disabled="disabled")
	article#chat
		h1 Chatting
		input#session-username(type="hidden", value=username)
		input#session-id(type="hidden", value=id)
		table.chat
			tr
				td(rowspan="2", style="width: 20%;")
					h3#username-label Masukkan Nama :
					input#username-login(type="text")
					button.tombol#tombol-login Login
					button.tombol#tombol-logout(style="display: none;") Logout
				td(colspan="2", style="height: 400px;")
					div#chat-body
			tr
				td(style="width: 55%;")
					textarea#chat-text(name="hello", rows="3", style="width: 99%;", disabled)
				td
					button#chat-kirim(disabled) Kirim
	article#about
		section.about
			h1 Aldi Nur Hendra
			p Asisten Lab Praktikum Pemrograman Berbasis Objek
			b Java :
			pre System.out.println("Hello, world!");
		section.about
			h1 Rofi Indrawan
			p Asisten Lab Praktikum Pemrograman Web
			b HTML :
			pre &lt;p&gt;Hello, world!&lt;/p&gt;
		section.about
			h1 Fania Prita Irgadanti
			p Asisten Lab Praktikum Basis Data
			b SQL :
			pre SELECT * FROM hello_world;
	aside
		div#chat-panel
		div
			div#chat-input
			div#chat-send
